s = "AABBC"


def compressString(s, nl):
    print
    
def expandString(s, nl):
    i = len(s) - 1
    s = list(s + " " * (nl - len(s)))
    h = len(s) - 1
    
    prev = None
    c = 0
    while i >= 0:
        if prev is None:
            prev = s[i]
            
        
        
        i -= 1
        print
    
    return "".join(s)
    

def getFinalCount(s):
    prev = None
    res = 0
    for i in s:
        if prev is None:
            prev = i
            c = 1
        elif prev == i:
            c += 1
        else:
            res = res + 1 + len(str(c))
            prev = i
            c = 0
    else:
        res = res + 1 + len(str(c))
    
    
            
    return res

nl = getFinalCount(s)

if nl > len(s):
    print expandString(s, nl)
else:
    print compressString(s, nl)





            
        
        
