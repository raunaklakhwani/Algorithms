from heapq import heappush,heappop
for _ in xrange(input()):
    N,G = map(int,raw_input().split())
    li = map(int,raw_input().split())
    li.sort(reverse = True)
    t = 0
    heap = []
    for i in xrange(N):
        #print heap
        if len(heap) == 2:
            t = heappop(heap)
            
        
        if t + li[i] > G:
            print "NO"
            break
        else:
            heappush(heap,t + li[i])
        
    else:
        print "YES"
        
        
        