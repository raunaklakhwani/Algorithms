


langDict = {}
langDict["java"] = "3"
langDict["cpp"] = "3"
langDict["csharp"] = "3"


lang = raw_input()
input = ""
code = ""
code = ""
input=[]
while True:
    try:
        s = raw_input()
        if s == "#" * 15:
            break
        code += s
    except EOFError:
        break
         
while True:
    try:
        s = raw_input()
        input.append(s)
    except EOFError:
        break
     
     

