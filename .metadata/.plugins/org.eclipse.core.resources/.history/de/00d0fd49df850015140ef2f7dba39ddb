if __name__ == '__main__':
    with open("fourth.txt", "w+") as f:
        for t in xrange(input()):
            out = "Case #" + str(t + 1) + ":" + "\n"
            f.write(out)
            r,c = map(int,raw_input().split())
            mat = []
            
            for i in range(r):
                mat.append(c * ['0'])
            for i in range(r):
                temp = raw_input()
                for j in range(c):
                    mat[i][j] = temp[j]
            q = int(raw_input())
            while q!=0:
                q -= 1
                qu = raw_input().split()
                if qu[0] == 'Q':
                    l = check(mat)
                    #print(l)
                    f.write(str(l)+'\n')
                else:
                    mat[int(qu[1])][int(qu[2])] = qu[3]
            
            
            