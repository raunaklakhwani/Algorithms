def pr(mat):
    for h in range(len(mat)):
        print(mat[h])
    print()

def bfs(mat, ch, i, j):
    if i == len(mat) or j == len(mat[0]) or i == -1 or j == -1:
        return
    if mat[i][j] == '0':
        return
    if mat[i][j] == '1' and ch[i][j] == 0:
        ch[i][j] = 1
        bfs(mat, ch, i+1, j)
        bfs(mat, ch, i-1, j)
        bfs(mat, ch, i, j+1)
        bfs(mat, ch, i, j-1)

def check(mat):
    ch = []
    for i in range(r):
        ch.append(c * [0])
    count = 0
    
    for i in range(len(mat)):
        for j in range(len(mat[0])):
            if mat[i][j] == '1' and ch[i][j] == 0:
                count += 1
                bfs(mat, ch, i, j)
    return count
    
