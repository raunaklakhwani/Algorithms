'''
2
ababa
aba
vnssnssnssnssnssnssnssnssnssnssnssnssnssnssnssnssn
nssnssns
'''


for _ in xrange(input()):
    ans = 0
    s = raw_input()
    z = raw_input()
    found = False
    ls = len(s)
    lz = len(z)
    for i in xrange(ls):
        if i + lz >= ls:
            break
        
        if s[i:i+lz] == z:
            ans += 1
            print "found"
            