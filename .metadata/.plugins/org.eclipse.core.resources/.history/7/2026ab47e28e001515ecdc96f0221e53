'''
2 10
'''
from _collections import defaultdict

N, M = map(int, raw_input().split())




def recurse(N, cache, ans, index, M):
    if index == N + 1:
        return ans % M
    
    for key in cache.keys():
        a, b = key[0], key[1]
        if index ^ a <= b:
            cache[(index ^ a, b)] += 1
            ans = (ans + 1) % M
        
        if a <= index ^ b:
            cache[(a, index ^ b)] += 1
            ans = (ans + 1) % M
            
        if a <= index:
            cache[(a, index)] += 1
            ans = (ans + 1) % M
        
        if b <= index:
            cache[(b, index)] += 1
            ans = (ans + 1) % M
            
            
    return recurse(N, cache, ans, index + 1, M)

cache = defaultdict(int)
cache[(0, 0)] = 1
print recurse(N, cache, 1, 1, M)
print cache
    
    
    
    
