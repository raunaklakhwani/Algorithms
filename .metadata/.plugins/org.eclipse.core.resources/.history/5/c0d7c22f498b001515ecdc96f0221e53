for _ in xrange(input()):
    N,d = map(int,raw_input().split())
    a = [0] + map(int,raw_input().split()) + [0]
    
    coords = []
    graph = [[0] * N for i in xrange(N)]
    for i in xrange(N):
        x,y = map(int,raw_input().split())
        
        j = len(coords) - 1
        while j >= 0:
            
            j = j - 1
        
        
        coords.append((x,y))
        
    
    
    
    