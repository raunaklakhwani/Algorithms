li = [0, 1, 1, 0, 1, 2, 1, 2, 0, 0, 0, 1]
def swap(li, a, b):
    li[a], li[b] = li[b], li[a]
    
def partition(inp, low, high):
    pivot = inp[low]
    lt = eq = gt = low + 1
    for i in xrange(low + 1, high + 1):
        if inp[i] < pivot:
            inp[lt], inp[eq], inp[gt] = inp[gt], inp[lt], inp[eq]
            lt += 1
            eq += 1
            gt += 1
        elif inp[i] == pivot:
            inp[eq], inp[gt] = inp[gt], inp[eq]
            eq += 1
            gt += 1
        else:
            gt += 1
    
    inp[low], inp[lt-1] = inp[lt-1], inp[low]
    return lt - 1, eq

        
print li
    
    
