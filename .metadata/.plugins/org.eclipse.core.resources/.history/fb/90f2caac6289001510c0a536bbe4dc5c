from collections import deque
words = ["a","b","ba","bca","bda","bdca"]

words.sort(key = lambda item:len(item),reverse = True)


def recurse(words,index,cache):
    if cache.get(index) is not None:
        return cache[index]
    else:
        l = list(words[index])
        for i in xrange(len(l)):
            temp = l[i]
            del l[i]
        

