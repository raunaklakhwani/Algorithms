

def recurse(N,m,last):
    if N == 0 and m == 0:
        return 1
    elif m > N:
        return 0
    elif N <= 0:
        return 0 
    elif N == m:
        return 1
    elif m == 0:
        return 0
    else:
        ans = 0
        for i in xrange(1,last + 1):
            if i <= last:
                res = recurse(N - i, m - 1,i)
                ans = ans + res
    return ans


N = 5
print recurse(N, 4,N - 1)