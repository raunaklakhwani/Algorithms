from pprint import pprint

def funPal(s):
    N = len(s)
    dp = [[0] * N for _ in xrange(N)]
    
    for i in xrange(N):
        dp[i][i] = 1
        
        
    for l in xrange(2, N):
        for i in xrange(N):
            j = i + l - 1
            if j >= N:
                break
            
            if s[i] == s[j]:
                dp[i][j] = 1 + dp[i + 1][j - 1]
                
    pprint(dp)
    
    ans = 0
    for i in xrange(N):
        ans = max(ans,dp[0][i] * fp[i+1][-1])
        
        
                
                
                
if __name__ == '__main__':
    s = "ABCBA"
    funPal(s)
                
     
                
    
                
            
    
    
     
