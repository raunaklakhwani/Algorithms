from collections import defaultdict
import re
if __name__ == '__main__':
    worddict = defaultdict(int)
    with open("corpus.txt") as f:
        while True:
            try:
                line = f.readline()
                if len(line):
                    for word in re.split("\W+",line.upper()):
                        worddict[word] += 1
                else:
                    break
            except EOFError:
                break
            
