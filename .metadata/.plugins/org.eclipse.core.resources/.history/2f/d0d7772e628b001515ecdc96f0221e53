'''
2
4
1 3 5 4
6
10 4 7 2 1 3
'''

def swap(li,i,j):
    li[i],li[j] = li[j],li[i]



for _ in xrange(input()):
    N = input()
    li = map(int,raw_input().split())
    currTime = 0
    li.sort()
    ans = 0
    for i in xrange(N):
        if li[i] >= currTime:
            currTime += li[i]
            ans += 1
            
        
    print ans
    
    
            
            
            
    
    
    
    
    
            