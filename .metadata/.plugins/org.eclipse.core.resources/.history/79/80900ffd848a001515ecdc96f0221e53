class Solution(object):
    def hIndex(self, li):
        """
        :type citations: List[int]
        :rtype: int
        """
        N = len(li)
        l = 0
        r = N - 1
        
        while l <= r:
            print l,r
            mid = l + ((r - l) >> 1)
            remaining = N - mid
            if li[mid] >= remaining:
                if li[mid-1] >= remaining + 1:
                    r = mid
                else:
                    return mid
            else:
                l = mid + 1
        
        
if __name__ == '__main__':
    s = Solution()
    print s.hIndex([0,1,3,5,6])
    print s.hIndex([1,2,2,6,7])