from collections import Counter,defaultdict
class Solution(object):
    def findSubstring(self, s, words):
        """
        :type s: str
        :type words: List[str]
        :rtype: List[int]
        """
        
        lw = len(words[0])
        N = len(s)
        
        wdict = Counter(words)
        res = []
        for i in xrange(N):
            if i + lw >= N:
                break
            tdict = defaultdict(int)
            for j in xrange(i,N,lw):
                temp = s[j:j+lw]
                if temp in words:
                    if tdict[temp] <= wdict[temp]:
                        tdict[temp] += 1
                        for key in wdict:
                            if wdict[key] != tdict[key]:
                                break
                        else:
                            res.append(i)
                            break
                else:
                    break
        return res
                            
                        
                    
                
        