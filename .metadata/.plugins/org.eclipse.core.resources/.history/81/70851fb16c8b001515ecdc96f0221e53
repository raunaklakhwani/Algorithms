'''
2
7
5 8 13 21 34 55 89
8
1 1 1 1 1 1 1 1
'''
from collections import defaultdict
for _ in xrange(input()):
    N = input()
    li = map(int, raw_input().split())
    li.sort()
    
    dp = [defaultdict(list) for i in xrange(N)]
    
    for i in xrange(N):
        dp[i][1] = [li[i]]
        for j in xrange(i - 1, -1, -1):
            cache = dp[j]
            
            
