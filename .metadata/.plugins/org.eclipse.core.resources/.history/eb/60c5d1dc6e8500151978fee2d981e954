'''
5
acb
abc
dce
edc
aab
'''

from collections import defaultdict
N = input()
h = defaultdict(int)
count = 1
c = set([])
for _ in xrange(N):
    s = sorted(list(raw_input()))
    ls = "".join(s)
    if h.get(ls) is not None:
        h[ls] = 1
    else:
        h[ls] += 1
        c.add(ls)


        
    