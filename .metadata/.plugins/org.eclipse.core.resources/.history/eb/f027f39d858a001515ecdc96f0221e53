class Solution(object):
    def hIndex(self, li):
        """
        :type citations: List[int]
        :rtype: int
        """
        if len(li) == 0:
            return 0
        
        N = len(li)
        l = 0
        r = N - 1
        
        while l < r:
            #print l,r
            mid = l + ((r - l) >> 1)
            remaining = N - mid
            if li[mid] >= remaining:
                if li[mid-1] >= remaining + 1:
                    r = mid
                else:
                    return remaining
            else:
                
                l = mid + 1
        
        if li[l] >= 1:
            return 1
        else:
            return 0
        
        
if __name__ == '__main__':
    s = Solution()
    print s.hIndex([0,1,3,5,6])
    print s.hIndex([1,2,2,6,7])
    print s.hIndex([11,15])