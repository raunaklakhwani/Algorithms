def compare(a,b):
    if len(a) < len(b):
        return 1
    elif len(a) < len(b):
        return -1
    else:
        if a > b:
            return 1
        else:
            return -1
        return (a > b)
    
 
minMax = ["abbc","abcdca","aaadaa"]    
print minMax
minMax.sort(cmp=compare)
print minMax