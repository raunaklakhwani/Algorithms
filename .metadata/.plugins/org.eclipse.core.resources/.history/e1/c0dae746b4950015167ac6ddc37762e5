c = 0
def recurse(index,res,size,main):
    global c
    if index == size:
        main.append("".join(res))
        c += 1
        return 
    for i in "ABC":
        try:
            res.append(i)
            x = "".join(res).index(f1)
            del res[-1]
        except ValueError:
            try:
                y = "".join(res).index(f2)
                del res[-1]
            except ValueError:
                recurse(index + 1, res, size,main)
                del res[-1]
            
    
                
                
                
if __name__ == '__main__':
    size = 3
    f1 = "AB"
    f2 = "BA"
    c = 0
    main = []
    recurse(0, [], size,main)
    print c
     
                
    
                
            
    
    
     
