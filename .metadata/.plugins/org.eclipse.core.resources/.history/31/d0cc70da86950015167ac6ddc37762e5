l = [1,2,1]
r = 2
N = len(l)


l.sort()



def recurse(res,main,index,r,l,N):
    if len(res) == r:
        main.append(res[:])
        return
    elif index == N:
        return
    i = index
    while index < N:
        #include
        temp = i
        res.append(l[index])
        while temp < N and l[index] == l[temp]:
            temp += 1
        
        recurse(res, main, temp, r, l, N)
        del res[-1]
        
         
        #exclude
        temp = i
        while temp < N and l[index] == l[temp]:
            temp += 1
        recurse(res, main, temp, r, l, N)
        
        i = temp
        

main = []
recurse([], main, 0, r, l, N)
print main
        
        
        
        