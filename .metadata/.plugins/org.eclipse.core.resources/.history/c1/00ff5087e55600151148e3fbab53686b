class UnionFind:
    def __init__(self, li):
        self.p = [-1] * len(li)

    def find(self, x):
        if self.p[x] >= 0:
            parent = self.p[x]
            result,size = self.find(parent)
            self.p[x] = result
            return (result,size)
        else:
            return (x,self.p[x])
        
    def union(self,a,b):
        print


for _ in xrange(1):
