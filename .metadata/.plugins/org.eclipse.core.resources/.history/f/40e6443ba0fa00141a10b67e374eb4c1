def getIndex(num):
    j = len(active) - 1
    while j >= 0:
        if len(active[j]) > 0 and num > active[j][-1]:
            break
        j = j - 1    
   
N = int(raw_input())
active = []
for i in xrange(N):
    num = int(raw_input())
    getIndex(num)
    print active
print len(active[-1])

    