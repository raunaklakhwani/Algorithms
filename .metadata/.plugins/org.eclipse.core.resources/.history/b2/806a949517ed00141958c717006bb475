
num_of_disks = 3


def moveDisk(num,source,aux,destination):
    if num > 0:
        moveDisk(num-1, source, destination, aux)
        print source ," --> ", destination
        moveDisk(num-1, aux, source, destination)
        


if __name__ == '__main__':
    moveDisk(num_of_disks, "S", "A", "D")