'''
2
6 0
10
102
120
1032
1212103
1999999
2 0
10
1
'''

def getAns(num,x):
    if num == 0:
        if num >= 0 and num <= x:
            return 1
        else:
            return 0
        
    c = 0
    while num:
        a = num % 10
        if a <= x:
            c += 1
        num = num / 10
    
    return c

for _ in xrange(input()):
    N,x = map(int,raw_input().split())
    cache = {}
    ans = 0
    for _ in xrange(N):
        element = input()
        if cache.get(element) is not None:
            ans += cache[element]
        else:
            c = getAns(element, x)
            