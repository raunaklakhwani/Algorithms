from compiler.ast import Node
from _collections import deque
class Node:
    def __init__(self,data,end,left,equal,right):
        self.data = data
        self.end = end
        self.left = left
        self.equal = equal
        self.right = right

def levelOrder(root):
    print "Level order = "
    print "_________________________"
    if root:
        q = deque([root,None])
        while len(q):
            
    print "_________________________"
    

def createTernarySearchTree(inp):
    root = None
    if len(inp):
        for s in inp:
            root = addNode(root, s)
    return root

def addNode(root,s):
    if root is None:
        if len(s) == 1:
            root = Node(s[0],1,None,None,None)
            return root
        elif len(s) == 0:
            return root
        else:
            root = Node(s[0],0,None,None,None)
            root.equal = addNode(root,s[1:])
            return root
    else:
        if len(s) == 0:
            root.end = 1
            return root
        else:
            if s[0] > root.data:
                root.right = addNode(root.right, s)
            elif s[0] < root.data:
                root.left = addNode(root.left,s)
            else:
                root.equal = addNode(root.equal, s[1:])
            
            
    
        
if __name__ == '__main__':
    root = createTernarySearchTree(["cat","bug","cats","up"])