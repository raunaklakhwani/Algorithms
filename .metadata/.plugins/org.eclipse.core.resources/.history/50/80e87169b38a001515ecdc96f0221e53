class Solution(object):
    def findKthLargest(self, nums, k):
        """
        :type nums: List[int]
        :type k: int
        :rtype: int
        """
        
        def swap(i,j,nums):
            print i,j
            nums[i],nums[j] = nums[j],nums[i]
        
        def partition(nums,s,e):
            key = nums[s]
            start = s + 1
            
            j = start + 1
            for i in xrange(start,e):
                if key < nums[i]:
                    swap(i,j,nums)
                    j += 1
            else:
                swap(i-1,s)
            
            print nums
            
        partition(nums, 0, len(nums))
            
        
            
                    
                
                
                
        
        
if __name__ == '__main__':
    s = Solution()
    s.findKthLargest([5,8,7,1,3,4], 3)