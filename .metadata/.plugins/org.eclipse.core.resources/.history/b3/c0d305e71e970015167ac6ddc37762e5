
def myformat(s, l):
    o = -1
    c = -1
    v = [False] * len(l)
    N = len(s)
    for i in xrange(N):
        try:
            o = s.index("{", o + 1, len(s))
            c = s.index("}", o + 1, len(s))
            if s[o + 1:c].isdigit():
                num = int(s[o + 1:c])
                if num < len(l):
                    if not v[num]:
                        v[num] = True
                        print l[num]
                    else:
                        print num
                else:
                    print num
            elif s[o + 1:c].strip() == "":
                start = 0
                while start < len(l):
                    if not v[start]:
                        v[start] = True
                        print l[start]
                        break
                    start += 1
                else:
                    print s[o + 1:c]
            else:
                print s[o + 1:c]
                
            
        except ValueError:
            return
    
    
class Solution(object):
    def reverseWords(self, s):
        """
        :type s: str
        :rtype: str
        """
        
        def reverseWord(s, i, j, p=0):
            while i < j:
                s[i],[j] = s[j],s[i]
                i += 1
                j -= 1
                
                
            
        s = list(s)    
        start = -1
        e = 0
        N = len(s)
        p = 0
        while e < N:
            if s[e] == " ":
                if start != -1:
                    reverseWord(s, start, e - 1, p)
                    start = -1
                    p = e + 1
            else:
                if start == -1:
                    start = e
            e += 1
        else:
            if start != -1:
                reverseWord(s, start, e - 1, p)
                start = -1
                p = e + 1
                
        reverseWord(s, 0, len(s) - 1)
        return "".join(s)
                
            
            
        
if __name__ == '__main__':
    s = Solution()
    print s.reverseWords("ONE    TWO")
