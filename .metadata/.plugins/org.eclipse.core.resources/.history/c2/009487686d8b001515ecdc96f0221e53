'''
2
7
5 8 13 21 34 55 89
8
1 1 1 1 1 1 1 1
'''
from collections import defaultdict
for _ in xrange(input()):
    N = input()
    li = map(int, raw_input().split())
    li.sort()
    
    dp = [defaultdict(set) for i in xrange(N)]
    
    m = 2
    for i in xrange(N):
        dp[i][1] = [li[i]]
        for j in xrange(i - 1, -1, -1):
            cache = dp[j]
            for k in cache.keys():
                if k == 1:
                    for t in cache[k]:
                        dp[i][k + 1].add(t + li[i])
                else:
                    for t in cache[k]:
                        if t == li[i]:
                            dp[i][k+1].add(li[j] + li[i])
    
    print dp
                        
