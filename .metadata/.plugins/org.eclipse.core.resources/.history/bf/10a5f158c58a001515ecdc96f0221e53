if len(res) and res[-1][1] != sh: 
                        res.append((a2,sh))
                    elif not len(res):
                        res.append((a2,sh))
                    
                    
                        
                
                
                
                    
            
            
            
        
        
        
        
        
        
if __name__ == '__main__':
    s = Solution()
    print s.getSkyline([[2, 9, 10], [3, 7, 15], [5, 12, 12], [15, 20, 10], [19, 24, 8]])
    
